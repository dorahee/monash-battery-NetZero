  groups   = {Batteries},
  keywords = {distributed power generation;integer programming;linear programming;power generation scheduling;renewable energy sources;isolated microgrid;energy storage;probabilistic constraints;optimal scheduling mode;chance-constrained programming;readily solvable mixed integer linear programming formulation;general algebraic modeling system;discretized step transformation approach;CPLEX solver;spinning reserve probability constraints;Uncertainty;Probabilistic logic;Optimal scheduling;Spinning;Wind speed;Programming;Microgrids;Chance-constrained programming (CCP);discretized step transformation (DST);distributed energy resources (DERs);isolated microgrids;optimal scheduling;spinning reserves;uncertainties},
}

