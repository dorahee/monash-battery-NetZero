@Article{MohsenianRad2010a,
  author     = {A. H. Mohsenian-Rad and A. Leon-Garcia},
  journal    = {IEEE Transactions on Smart Grid},
  title      = {Optimal Residential Load Control With Price Prediction in Real-Time Electricity Pricing Environments},
  year       = {2010},
