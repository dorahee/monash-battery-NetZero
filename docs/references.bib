  keywords  = {ageing;battery storage plants;lithium compounds;power grids;power markets;pricing;profitability;scheduling;secondary cells;profitability concern;electricity market;computing optimal charging-dispatching scheduling;real-time price;battery aging nonlinear dependency;service profitability;peak shaving;frequency regulation;power grid;grid-integrated lithium-ion battery;residential battery;electric vehicle;electricity price forecast;battery degradation;battery energy storage smart scheduling;energy arbitrage;Batteries;State of charge;Degradation;Real-time systems;Aging;Context;Optimization;batteries;battery degradation;electric vehicle;grid-integration;scheduling},
}

@Article{Abdulla2018,
  author   = {K. {Abdulla} and J. {de Hoog} and V. {Muenzel} and F. {Suits} and K. {Steer} and A. {Wirth} and S. {Halgamuge}},
  journal  = {IEEE Transactions on Smart Grid},
  title    = {Optimal Operation of Energy Storage Systems Considering Forecasts and Battery Degradation},
  year     = {2018},
  issn     = {1949-3061},
  month    = {May},
  number   = {3},
  pages    = {2086-2096},
  volume   = {9},
  abstract = {Energy storage systems have the potential to deliver value in multiple ways, and these must be traded off against one another. An operational strategy that aims to maximize the returned value of such a system can often be significantly improved with the use of forecasting - of demand, generation, and pricing - but consideration of battery degradation is important too. This paper proposes a stochastic dynamic programming approach to optimally operate an energy storage system across a receding horizon. The method operates an energy storage asset to deliver maximal lifetime value, by using available forecasts and by applying a multi-factor battery degradation model that takes into account operational impacts on system degradation. Applying the method to a dataset of a residential Australian customer base demonstrates that an optimally operated system returns a lifetime value which is 160% more, on average, than that of the same system operated using a set-point-based method applied in many settings today.},
  doi      = {10.1109/TSG.2016.2606490},
  groups   = {Batteries},
  keywords = {dynamic programming;energy management systems;energy storage;stochastic processes;stochastic programming;stochastic dynamic programming approach;energy storage system;energy storage asset;maximal lifetime value;multifactor battery degradation model;operational strategy;Batteries;Degradation;Numerical models;Australia;Stochastic processes;Optimization;Energy storage;optimal operation;dynamic programming;forecasting;battery aging},
}

@InProceedings{Zhou2018,
